# Global environment configuration
# Copy this file to `.env` at the repo root and fill in real values.

# Supabase
SUPABASE_URL=https://<project>.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key   # preferred for server/edge fetches with RLS
# SUPABASE_ANON_KEY=your_anon_key                  # optional if RLS permits anon reads

# xAI API
XAI_API_KEY=your_xai_api_key

# X / Twitter OAuth (auth-agents)
X_CLIENT_ID=your_x_client_id
AUTH_SERVER_HOST=127.0.0.1
AUTH_SERVER_PORT=8000

# Models (optional overrides)
DEFAULT_MODEL=grok-4-1-fast-non-reasoning
IMAGE_MODEL=grok-imagine-v0p9

# CTR Prediction (optional)
CTR_ENSEMBLE_RUNS=10

